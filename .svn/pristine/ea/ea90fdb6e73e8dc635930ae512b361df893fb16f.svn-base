            android:exported="true" />
        <activity
            android:name="com.example.teamnugget.PolytechnicUI"
            android:exported="true" />
        <activity
            android:name="com.example.teamnugget.PolyCourses"
            android:exported="true" />
        <activity
            android:name="com.example.teamnugget.FilterSearch"
            android:exported="true" />
        <activity
            android:name="com.example.teamnugget.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>